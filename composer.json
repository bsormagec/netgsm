{
    "name": "clevel/netgsm-notification-channel",
    "description": "NetGSM Provider for Laravel Notification Channels",
    "homepage": "https://github.com/laravel-notification-channels/netgsm",
    "license": "MIT",
    "authors": [
        {
            "name": "Burak SormageÃ§",
            "email": "burak@sormagec.com",
            "homepage": "https://clevel.team",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/notifications": "~5.5 || ~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Netgsm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Netgsm\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
